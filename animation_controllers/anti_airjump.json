{
    "format_version": "1.16.0",
    "animation_controllers": {
      "controller.animation.player.anti_airjump": {
        "states": {
          "default": {
            "transitions": [
              {
                "open": "query.is_jumping == 1.0"
              }
            ],
            "on_exit": [
              "/execute @e[scores={aajmtoggle=1}] ~~~ /function atomic/modules/anti_airjump"
            ]
          },
          "open": {
            "transitions": [
              {
                "default": "query.is_jumping == 0.0"
              }
            ]
          }
        }
      }
    }
  }
